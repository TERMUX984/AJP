<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="test.person" table="Persons">
		<id name="Id" column="Persons_id">
		</id>
		<property name="Name" column="Persons_name"></property>
		<property name="Add" column="Person_add"></property>
		<property name="Age" column="Persons_age"></property>
	</class>
</hibernate-mapping>